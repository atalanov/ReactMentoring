{
    "extends": "airbnb",
    "rules":{
        "linebreak-style": "off",
        "no-console": "off",
        "indent": "off",
        "react/jsx-filename-extension": "off",
        "no-unused-vars": "off",
        "max-len": "off",
        "react/prop-types": "off",
        "function-paren-newline":"off",
        "no-underscore-dangle":"off"
    },
    "globals": {
        "test": true,
        "expect": true,
        "it": true,
        "describe": true
    },
    "env": {
        "browser": true
    }
}